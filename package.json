{
  "name": "@kuvali/ui",
  "version": "0.1.0-alpha",
  "//": "--- IMPORTANT: Using custom index.js as entry point for Polyfills ---",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "private": true,
  "author": "Tom <kuvali-js@proton.me>",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kuvali-js/core.git"
  },
  "bugs": {
    "url": "https://github.com/kuvali-js/core/issues"
  },
  "homepage": "https://kuvali-js.github.io",
  "scripts": {
    "start": "npx expo start",
    "//": "--- Clean start is recommended after dependency changes ---",
    "clean:start": "npx expo start -c",
    "//": "--- Ultimate Cleanup & Build Scripts ---",
    "clean:ios": "xcrun simctl uninstall booted com.anonymous.whitedog || true && watchman watch-del-all && rm -rf $TMPDIR/metro-cache && yes | npx expo prebuild --clean && npx expo run:ios",
    "clean:android": "adb uninstall com.anonymous.whitedog || true && watchman watch-del-all && rm -rf $TMPDIR/metro-cache && yes | npx expo prebuild --clean && npx expo run:android",
    "test": "jest",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "reset-project": "node ./scripts/reset-project.js"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "moduleNameMapper": {
      "^@kuvali/core$": "<rootDir>/core/index.ts"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@nozbe/watermelondb": "^0.28.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/cli": "^20.1.0",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@sentry/react-native": "~7.2.0",
    "@supabase/supabase-js": "^2.90.1",
    "@testing-library/jest-native": "^5.4.3",
    "expo": "~54.0.30",
    "expo-build-properties": "~1.0.10",
    "expo-constants": "~18.0.13",
    "expo-crypto": "^15.0.8",
    "expo-dev-client": "~6.0.20",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.21",
    "expo-secure-store": "~15.0.8",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "jest": "~29.7.0",
    "jest-expo": "~54.0.16",
    "loglevel": "^1.9.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-bootsplash": "^6.3.12",
    "react-native-element-dropdown": "^2.12.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-get-random-values": "~1.11.0",
    "react-native-nitro-modules": "^0.33.1",
    "react-native-quick-crypto": "^1.0.6",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.0",
    "@types/readable-stream": "^4.0.23",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "react-test-renderer": "^19.1.0",
    "typescript": "~5.9.2"
  }
}
