{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "types": ["expo", "node"],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strictPropertyInitialization": false,
    "paths": {
      "@kuvali/core/*": ["./core/*"],
      "@kuvali/core": ["./core/index.ts"],
      "@kuvali/translations/*": ["./core/i18n/translations/*"],
      "@kuvali/core-ui": ["./core/core-ui/index.ts"],
      "@services/*": ["./services/*"],
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
, "App.tsx"  ]
}
